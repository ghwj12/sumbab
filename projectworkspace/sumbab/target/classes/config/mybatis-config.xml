<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE configuration PUBLIC "-//mybatis.org//DTD Config 3.0//EN"
"http://mybatis.org/dtd/mybatis-3-config.dtd">

<configuration>
	<!-- mybatis 작동 규칙 정의 -->
	<settings>
		<setting name="cacheEnabled" value="false"/>
		<setting name="useGeneratedKeys" value="false"/>
		<setting name="mapUnderscoreToCamelCase" value="true"/>
	</settings>
	
	<typeAliases>
		<typeAlias alias="store" type="com.sumbab.project.model.store.Store"/>
		<typeAlias alias="store" type="com.sumbab.project.model.cafeCat.CafeCat"/>
		<typeAlias alias="store" type="com.sumbab.project.model.restCat.RestCat"/>
		<typeAlias alias="store" type="com.sumbab.project.model.reserve.Reserve"/>
		<typeAlias alias="store" type="com.sumbab.project.model.review.Review"/>
		<typeAlias alias="store" type="com.sumbab.project.model.tag.Tag"/>
	</typeAliases> 
	
</configuration>