<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE configuration PUBLIC "-//mybatis.org//DTD Config 3.0//EN"
"http://mybatis.org/dtd/mybatis-3-config.dtd">

<configuration>
	<!-- mybatis 작동 규칙 정의 -->
	<settings>
		<setting name="cacheEnabled" value="false"/>
		<setting name="useGeneratedKeys" value="false"/>
		<setting name="mapUnderscoreToCamelCase" value="true"/>
	</settings>
	
	<typeAliases>
		<typeAlias alias="store" type="store.Store"/>
		<typeAlias alias="restCat" type="store.restCat.RestCat"/>
		<typeAlias alias="cafeCat" type="store.cafeCat.CafeCat"/>
		<typeAlias alias="reserve" type="my.reserve.Reserve"/>
		<typeAlias alias="review" type="store.review.Review"/>
		<typeAlias alias="tag" type="store.review.tag.Tag"/>
	</typeAliases> 
	
</configuration>